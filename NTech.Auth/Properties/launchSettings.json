{
  "profiles": {
    "http": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:5096"
    },
    "https": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DB_DEFAULT_CONNECTION": "Test"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:7124;http://localhost:5096"
    },
    "Container (Dockerfile)": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
      "environmentVariables": {
        "ASPNETCORE_HTTPS_PORTS": "8081",
        "ASPNETCORE_HTTP_PORTS": "8080"
      },
      "publishAllPorts": true,
      "useSSL": true
    },
    "Proxy": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "http://auth.ntech.internal/",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "PROXY": "true"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:5096"
    }
  },
  "$schema": "https://json.schemastore.org/launchsettings.json"
}