# OpenIddict configuration
OPENID_SIGNING_KEY_LOCATION=/etc/keys/signing.key
OPENID_ENCRYPTION_KEY=MySuperSecretEncryptionKey123!
OPENID_AUTHORITY=https://auth.example.com

# Database connection (adjust to your database)
DEFAULT_CONNECTION_STRING=Server=db;Database=NTechAuthDb;User Id=sa;Password=YourStrong(!)Password;

# Default user account
DEFAULT_USERNAME=Admin
DEFAULT_PASSWORD=changeit123
DEFAULT_LASTNAME=Lastname
DEFAULT_FIRSTNAME=Firstname
DEFAULT_EMAIL=admin@example.com

# Docker configuration
DOCKER_PORT_HTTP=5000

# Host keys path
HOST_KEYS_PATH=/opt/keys

# Github
GITHUB_PAT=your_github_pat_here